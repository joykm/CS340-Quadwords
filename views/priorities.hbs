<h1>Priority Manager</h1>

<div>
    Note: Click the row to update/delete
</div>

{{!-- Handlebars call to add_project_modal.hbs partial --}}
{{> add_priority_modal}}

<table>
  <thead>
    <tr>
      <th scope="col">Priority ID</th>
      <th scope="col">Priority Type</th>
    </tr>
  </thead>
  <tbody>
    <tr class="get-priority">
      <td scope="row">1</td>
      <td class="get-priority">Low</td>
    </tr>
    <tr class="get-priority">
      <td scope="row">2</td>
      <td class="get-priority">Medium</td>
    </tr>
    <tr class="get-priority">
      <td scope="row">3</td>
      <td class="get-priority">High</td>
    </tr>
  </tbody>
</table>

{{!-- Handlebars call to update_project_modal.hbs partial --}}
{{> update_priority_modal}}