<div id="issuesAssignedUsersModal" aria-labelledby="exampleLabel" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleLabel">Assigned Users</h5>
               <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
          {{!-- Handlebars building dynamic table from app.get request--}}
          {{#each issue_assignments}}
            <hr>
            <p>Issue ID: {{issueID}} &nbsp &nbsp &nbsp Developer: {{firstName}} {{lastName}}</p>
            <hr>
          {{/each}}
      </div>
    </div>
  </div>
</div>
<script src="/scripts/issues_assigned_users.js"></script>